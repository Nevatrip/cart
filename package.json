        "@babel/register": "^7.0.0",
        "babel-core": "^7.0.0-0",
        "babel-eslint": "8.2.6",
        "babel-jest": "^23.4.2",
        "babel-loader": "~7.1.5",
        "chalk": "2.4.1",
        "clean-webpack-plugin": "~0.1.19",
        "cross-env": "~5.2.0",
        "css-loader": "1.0.0",
        "css-mqpacker": "~7.0.0",
        "cssnano": "^4.0.5",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.14.0",
        "enzyme-to-json": "^3.3.5",
        "eslint": "5.2.0",
        "eslint-plugin-react": "~7.10.0",
        "favicons-webpack-plugin": "~0.0.9",
