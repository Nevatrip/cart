        "react-datepicker": "^2.7.0",
        "react-dom": "^16.8.6",
        "react-redux": "^5.1.1",
        "react-redux-form": "^1.16.13",
        "react-router-dom": "^4.3.1",
        "react-router-redux": "^5.0.0-alpha.8",
        "react-transition-group": "~2.4.0",
        "redux": "^4.0.1",
        "redux-saga": "^0.16.2",
        "reset-css": "~4.0.1",
        "semantic-ui-css": "^2.4.1",
        "semantic-ui-react": "^0.86.0",
        "tz-lookup": "^6.1.18"
    },
